upstream gate_pars_upstream \{
	server unix:\/tmp\/${SERVICE_NAME}.sock\;
\}

server \{
	listen 80\;
	server_tokens off\;
	server_name ${SERVICE_IP_ADDRESS}\;
	fastcgi_read_timeout 900s\;

	location \/ \{
		include uwsgi_params\;
		uwsgi_pass unix:${SERVICE_DIR}\/${SERVICE_NAME}.sock\;
	\}

	location \/static \{
		root ${SERVICE_DIR}\;
	\}
\}
